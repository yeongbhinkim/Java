package main;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int arr[];

		int a = sc.nextInt();

		for (int i = 0; i < a; i++) {

			int s = sc.nextInt();
			arr = new int[s];

			double sum = 0;

			for (int j = 0; j < s; j++) {
				int d = sc.nextInt();
				arr[j] = d;
				sum += d;
			}
			double mean = (sum / s);
			double count = 0;

			for (int j = 0; j < s; j++) {
				if (arr[j] > mean) {
					count++;
				}
			}

			System.out.printf("%.3f%%\n", (count / s) * 100);
		}

	}
}
