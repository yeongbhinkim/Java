package first;

public class Practice {
	
	public static void main(String[] args) {
		int random = (int)(Math.random()*100)+1;
		System.out.println(random);
			
		if(random % 3 == 0) {
			System.out.println("3의배수");
		}else {
			System.out.println("3의배수가아님");
		}
		
		if(random % 2==0 && random % 3 ==0) {
			System.out.println("2와 3의 배수");
		}else {
			System.out.println("2와 3의 배수 아님");
		}
		if(random % 2 ==0 || random % 3 ==0) {
			System.out.println("2나 3의 배수");
		}else {
			System.out.println("2나 3의 배수아님");
		}
		if(!(random % 2 ==0)) {
			System.out.println("2의 배수");
		}else {
			System.out.println("2의 배수 아님");
		}
		if(random % 2 !=0) {
			System.out.println("2의 배수");
		}else {
			System.out.println("2의 배수아님");
		}
	}
}
		

	
