package main;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	
Scanner sc = new Scanner(System.in);
		
		String n = Integer.toString(sc.nextInt()*sc.nextInt()*sc.nextInt());
		
		
		for(int i=0; i<10; i++) {
			int count=0;
			for(int j=0; j< n.length(); j++) {
				if((n.charAt(j)-'0')==i) {
					
					count++;
				}
			}
			System.out.println(count);
		}

		
	
	}

}